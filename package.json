{
	"name": "letta-obsidian-plugin",
	"version": "1.3.1",
	"description": "Obsidian plugin for chatting with a stateful Letta AI agent",
	"main": "main.js",
	"scripts": {
		"dev": "node esbuild.config.mjs",
		"build": "tsc -noEmit -skipLibCheck && node esbuild.config.mjs production",
		"version": "node version-bump.mjs && git add manifest.json versions.json"
	},
	"keywords": [
		"obsidian",
		"letta",
		"ai",
		"agent",
		"chat"
	],
	"author": "Cameron",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^16.11.6",
		"@typescript-eslint/eslint-plugin": "5.29.0",
		"@typescript-eslint/parser": "5.29.0",
		"builtin-modules": "3.3.0",
		"esbuild": "0.17.3",
		"obsidian": "latest",
		"tslib": "2.4.0",
		"typescript": "^4.9.5"
	},
	"dependencies": {
		"@letta-ai/letta-client": "^0.0.68660"
	}
}
